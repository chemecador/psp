package main;

public class Domicilio {
	private String direccion;
	private char puerta;
	private int piso;
	private int CP;
	private String localidad;
	private String provincia;
	
	public Domicilio() {
		this.direccion = "";
		this.puerta = '';
		this.piso = 0;
		this.CP = 0;
		this.localidad = "";
		this.provincia = "";
	}
	public Domicilio(String direccion, char puerta, int piso,
			int CP, String localidad, String provincia) {
		this.direccion = "";
		this.puerta = '';
		this.piso = 0;
		this.CP = 0;
		this.localidad = "";
		this.provincia = "";
	}
}
