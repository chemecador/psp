package main;

public class Coche {
	private String marca;
	private String modelo;
	private String matricula;
	private int cilindrada;
	
	public Coche() {
		this.marca = "";
		this.modelo = "";
		this.matricula = "";
		this.cilindrada = 0;
	}
	public Coche(String marca, String modelo, String matricula,
			int cilindrada) {
		this.marca = marca;
		this.modelo = modelo;
		this.matricula = matricula;
		this.cilindrada = cilindrada;
	}
}
